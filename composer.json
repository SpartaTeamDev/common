{
  "name": "chaos/common",
  "type": "library",
  "description": "The Chaos Library",
  "keywords": ["ntd1712", "chaos"],
  "homepage": "http://taingheso.com",
  "license": "ntd1712",
  "authors": [
    {"name": "Dzung Nguyen", "email": "ntd1712@hotmail.com"}
  ],
  "require": {
    "php": ">=5.4",
    "hassankhan/config": "dev-develop",
    "league/container": "1.3.*",
    "tombzombie/dice": "dev-v2.0-PHP5.4-5.5",
    "doctrine/orm": "dev-master",
    "zendframework/zend-servicemanager": "dev-master",
    "zendframework/zend-db": "dev-master",
    "zendframework/zend-http": "dev-master",
    "zendframework/zend-json": "dev-master",
    "zendframework/zend-filter": "dev-master",
    "zendframework/zend-validator": "dev-master",
    "zendframework/zend-i18n": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "dev-master"
  },
  "autoload": {
    "files": [
      "src/Chaos/constants.php",
      "src/Chaos/functions.php"
    ],
    "psr-4": {
      "Chaos\\": "src/Chaos/"
    }
  },
  "archive": {
    "exclude": ["!vendor", "tests", "*phpunit.xml", ".travis.yml", "build.xml", "build.properties", "composer.phar"]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}